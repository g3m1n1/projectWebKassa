@model IEnumerable<projectWebKassa.DAL.prijs>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Prijs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.product.Naam)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Prijs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.product.Naam)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PrijsId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PrijsId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PrijsId })
        </td>
    </tr>
}

</table>
